<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<h1>Listing Users</h1>
<table>
  <tr>
    <th>ID:</th>
    <th>Name:</th>
    <th>Created At:</th>
    <th>Updated At:</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.name %></td>
      <td><%= user.created_at %></td>
      <td><%= user.updated_at %></td>
    </tr>
  <% end %>
</table>
